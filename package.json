{
  "name": "webuild",
  "description": "Website for https://webuild.community/",
  "version": "2.0.0",
  "dependencies": {
    "classnames": "^2.2.6",
    "dayjs": "^1.8.26",
    "gatsby-plugin-manifest": "2.9.1",
    "gatsby-plugin-offline": "3.7.1",
    "gatsby-plugin-postcss": "^3.4.0",
    "gatsby-plugin-preact": "^4.4.0",
    "gatsby-plugin-react-helmet": "3.7.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-source-filesystem": "^2.8.1",
    "gatsby-transformer-remark": "^2.13.1",
    "gatsby": "2.29.2",
    "preact-render-to-string": "^5.1.12",
    "preact": "^10.5.7",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react": "17.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss-transitions": "^2.2.0",
    "tailwindcss": "1.4.6",
    "typeface-barlow": "^0.0.71"
  },
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze": "yarn analyze:lint && yarn analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prettify": "prettier -c --write \"*/**\""
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.5",
    "eslint": "7.16.0",
    "gatsby-plugin-eslint": "2.0.8",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{json,css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "volta": {
    "node": "14.15.3",
    "yarn": "1.22.10"
  }
}
