{
  "name": "webuild",
  "description": "Website for https://webuild.community/",
  "version": "2.0.0",
  "dependencies": {
    "classnames": "^2.2.6",
    "dayjs": "^1.8.26",
    "gatsby": "2.21.23",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-offline": "3.2.1",
    "gatsby-plugin-postcss": "^2.3.1",
    "gatsby-plugin-preact": "^4.0.0",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-remark": "^2.8.7",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.1.8",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss-transitions": "^2.2.0",
    "typeface-barlow": "^0.0.71"
  },
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze": "yarn analyze:lint && yarn analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prettify": "prettier -c --write \"*/**\""
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.4",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-react": "7.19.0",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-netlify": "^2.3.13",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "tailwindcss": "1.4.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
